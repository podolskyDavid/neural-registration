      \strng{namehash}{98dd5f5f942b82b80020be980d0dbe26}
      \strng{fullhash}{637af4507dbde90878ba2394b647fefa}
      \strng{fullhashraw}{637af4507dbde90878ba2394b647fefa}
      \strng{bibnamehash}{637af4507dbde90878ba2394b647fefa}
